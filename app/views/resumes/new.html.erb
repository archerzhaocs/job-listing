<div class="col-md-4 col-md-offset-4">
  <h2>上传简历</h2>

  <hr>

  <%= simple_form_for [@job, @resume] do |f| %>

  <div class="form-resume">
    <%= f.input :content, input_html: {class: "form-control"} %>
    <%= f.input :attachment, input_html: {class: "form-control"} %>
  </div>
  <%= f.submit "Submit", class: "btn btn-primary", data: {disable_with: "Submitting...."} %>
  <% end %>

</div>
